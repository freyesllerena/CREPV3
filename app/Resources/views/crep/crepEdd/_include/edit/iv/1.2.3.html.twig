{% form_theme form 'crep/crepEdd/Form/collections.form.html.twig' %}


<div class="row" id="autresCompetencesManagerialesCollection" data-prototype="{{ form_widget(form.autresCompetencesManageriales.vars.prototype)|e('html_attr') }}">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <thead>
            <tr>
                <th style="width: 30%">Autres compétences éventuellement  démontrées par l’agent</th>
                <th style="width: 15%" class="text-center">Exceptionnelle</th>
                <th style="width: 10%" class="text-center">Forte</th>
                <th style="width: 10%" class="text-center">Assez forte</th>
                <th style="width: 15%" class="text-center">A développer</th>
                <th style="width: 12%" class="text-center">Non pertinent</th>
                <th style="width: 6%" class="text-center"></th>
            </tr>
            </thead>
            <tbody>
            {% for autreCompetenceManageriale in form.autresCompetencesManageriales %}
                {{ form_widget(autreCompetenceManageriale) }}
            {%  endfor %}
            </tbody>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <button class="btn btn-primary add_collection_link CrepEddAutresCompetencesManageriales pull-right">
            <i class="fa fa-plus"></i> Ajouter
        </button>
    </div>
</div>
