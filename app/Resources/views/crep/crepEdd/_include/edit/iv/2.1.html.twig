{% form_theme form 'crep/crepEdd/Form/collections.form.html.twig' %}


<div class="row" id="competencesTransversesDetenuesCollection"
     data-prototype="{{ form_widget(form.competencesTransversesDetenues.vars.prototype)|e('html_attr') }}">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <thead>
            <tr>
                <th style="width: 40%" class="text-center">Compétences détenues non requises sur le poste actuellement
                    occupé
                </th>
                <th style="width: 20%" class="text-center">Exceptionnelle</th>
                <th style="width: 10%" class="text-center">Forte</th>
                <th style="width: 10%" class="text-center">Assez forte</th>
                <th style="width: 15%" class="text-center">A développer</th>
                <th style="width: 5%" class="text-center"></th>
            </tr>
            </thead>
            <tbody>
            {% for competenceTransverseDetenue in form.competencesTransversesDetenues %}
                {{ form_widget(competenceTransverseDetenue) }}
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <button class="btn btn-primary add_collection_link CrepEddCompetencesTransversesDetenues pull-right">
            <i class="fa fa-plus"></i> Ajouter
        </button>
    </div>
</div>


