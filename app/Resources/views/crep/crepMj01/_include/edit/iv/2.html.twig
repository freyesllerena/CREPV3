{% form_theme form 'crep/crepMj01/Form/collections.form.html.twig' %}

<div class="row" id="objectifsFutursCollection" data-prototype="{{ form_widget(form.objectifsFuturs.vars.prototype)|e('html_attr') }}">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Descriptif des objectifs</th>
                    <th>Condition de mise en œuvre <br><small>(délais et moyens nécessaires)</small></th>
                    <th width="2%"></th>
                </tr>
            </thead>
            <tbody>
                {% for objectif in form.objectifsFuturs %}
                    {{ form_row(objectif) }}
                {%  endfor %}
            </tbody>
        </table>
    </div>
</div>
<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <button class="btn btn-primary add_collection_link pull-right">
            <i class="fa fa-plus"></i> Ajouter
        </button>
    </div>
</div>



