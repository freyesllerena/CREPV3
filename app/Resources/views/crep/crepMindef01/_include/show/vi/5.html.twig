<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <thead>
            <tr>
                <th>Typologie</th>
                <th>Libellé de la formation</th>
                <th>Code de la formation</th>
                <th class="text-center">Priorité</th>
                <th class="text-center">Niveau SAME</th>
                <th class="text-center">CPF</th>
            </tr>
            </thead>
            <tbody>
            {% for formationDemandeeAgent in crep.formationsDemandeesAgent %}
                <tr>
                    <td>{{ formationDemandeeAgent.typologie|selectTypologieFormationCrepMindef01 }}</td>
                    <td>{{ formationDemandeeAgent.libelle }}</td>
                    <td>{{ formationDemandeeAgent.code }}</td>
                    <td class="text-center">{{ formationDemandeeAgent.priorite }}</td>
                    <td class="text-center">{{ formationDemandeeAgent.niveauSame|echelleNiveauSameCrepMindef01 }}</td>
                    <td class="text-center">{{ formationDemandeeAgent.dif|ouiNon }}</td>
                </tr>
            {%  endfor %}
            </tbody>
        </table>
    </div>
</div>
