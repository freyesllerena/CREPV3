{% form_theme form 'crep/crepMindef01/_include/edit/ii/2.form.html.twig' %}

<div class="row" id="objectifsEvaluesCollection" data-prototype="{{ form_widget(form.objectifsEvalues.vars.prototype)|e('html_attr') }}">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <thead>
            <tr>
                <th>Rappel de l'objectif</th>
                <th>Analyse des r√©sultats</th>
                <th width="205px">Atteinte des objectifs</th>
                <th width="25px"></th>
            </tr>
            </thead>
            
            <tbody>
            {% for objectif in form.objectifsEvalues %}
                <tr>
                    <td>{{ form_row(objectif.libelle,{'attr': {'maxlength': '4096'}}) }}</td>
                    <td>{{ form_row(objectif.resultat,{'attr': {'maxlength': '4096'}}) }}</td>
                    <td>{{ form_row(objectif.resultatObtenu) }}</td>
                    <td><button class="btn btn-danger btn-xs remove-from-collection"><i class="fa fa-close"></i></button></td>
                </tr>
            {% endfor %}
            </tbody>
            
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <button class="btn btn-primary add_collection_link objectifsEvalues pull-right">
            <i class="fa fa-plus"></i> Ajouter
        </button>
    </div>
</div>
