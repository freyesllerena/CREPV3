{% form_theme form 'crep/crepMindef01/Form/App_form_crepMindef01.html.twig' %}

<div class="row" id="formationsDemandeesAgentCollection" data-prototype="{{ form_widget(form.formationsDemandeesAgent.vars.prototype)|e('html_attr') }}">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <tbody>
            {% for formation in form.formationsDemandeesAgent %}
                {{ form_widget(formation, {'attr': {'class': 'fieldCollection formation-autocomplete'}}) }}
            {% endfor %}
            </tbody>
        </table>
