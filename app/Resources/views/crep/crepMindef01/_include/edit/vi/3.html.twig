{% form_theme form 'crep/crepMindef01/Form/App_form_crepMindef01.html.twig' %}

<div class="row" id="formationsDemandeesAdministrationCollection" data-prototype="{{ form_widget(form.formationsDemandeesAdministration.vars.prototype)|e('html_attr') }}">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <tbody>
                {% for formation in form.formationsDemandeesAdministration %}
                    {{ form_widget(formation, {'attr': {'class': 'fieldCollection formation-autocomplete'}}) }}
                {% endfor %}
            </tbody>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <button class="btn btn-primary add_collection_link formationsDemandeesAdministration pull-right">
            <i class="fa fa-plus"></i> Ajouter
        </button>
    </div>
</div>