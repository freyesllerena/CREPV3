{% form_theme form 'crep/crepMj02/Form/collections.form.html.twig' %}

<div class="row" id="formationsAnneeEcouleeCollection" data-prototype="{{ form_widget(form.formationsAnneeEcoulee.vars.prototype)|e('html_attr') }}">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <tbody>
            {% for formation in form.formationsAnneeEcoulee %}
                {{ form_widget(formation) }}
            {%  endfor %}
            </tbody>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <button class="btn btn-primary add_collection_link formationsAnneeEcoulee pull-right">
            <i class="fa fa-plus"></i> Ajouter
        </button>
    </div>
</div>
