{% form_theme form 'crep/crepMso3/_include/edit/i/2.form.html.twig' %}

<div class="row" id="objectifsEvaluesCollection" data-prototype="{{ form_widget(form.objectifsEvalues.vars.prototype)|e('html_attr') }}">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <table class="table table-bordered">
            <thead>
	            <tr>
	                <th width="40%">Rappel de l'objectif</th>
	                <th width="220px">Réalisation</th>
	                <th width="40%">Analyse des résultats et enseignements tirés</th>
	                <th width="25px"></th>
	            </tr>
            </thead>
            
            <tbody>
            {% for objectif in form.objectifsEvalues %}
                 {{ form_row(objectif) }}
            {% endfor %}
            </tbody>
            
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <button class="btn btn-primary add_collection_link objectifsEvalues pull-right">
            <i class="fa fa-plus"></i> Ajouter
        </button>
    </div>
</div>
