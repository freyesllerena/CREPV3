{% if crep.agent and not crep.agent.sansAh %}
    <div class="row form-group">
        <div class="col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">Date</label>
            <div class="col-md-9 col-sm-9 col-xs-12 well well-xs">
                {% if crep.dateVisaAh %}{{ crep.dateVisaAh|date('d/m/Y H:i') }}{% endif %}
            </div>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">Nom</label>
            <div class="col-md-9 col-sm-9 col-xs-12 well well-xs">
                {% if crep.ahSignataire %}
                    {{ crep.ahSignataire | identite }}
                {% endif %}
            </div>
        </div>
    </div>
{% endif %}
