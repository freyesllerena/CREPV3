{% form_theme form 'crep/crepDgac/Form/App_form_crepDgac_12.html.twig' %}

<div class="row form-group">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<label><u>Par l'agent </u></label>
	</div>
</div>
<div class="row form-group">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<label>Quels sont les réalisations et les résultats obtenus par
			l'agent ?</label>
        {{ form_widget(form.obsAgentObjectifsEvalues,{'attr': {'maxlength': '4096'}}) }}
	</div>
</div>
<div class="row form-group">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<label>Y a-t-il des dossiers marquants non définis initialement
			(dans les objectifs de l'année écoulée et/ou dans la fiche de
			poste) dans lesquels l'agent s'est investi ?</label>
        {{ form_widget(form.autresObservationsAgent,{'attr': {'maxlength':'4096'}}) }}
	</div>
</div>
<div class="row form-group">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<label>Difficultés particulières (éventuelles)</label>
        {{ form_widget(form.commentaireResultatsAgent,{'attr': {'maxlength': '4096'}}) }}
	</div>
</div>
