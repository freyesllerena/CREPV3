{% form_theme form 'crep/crepDgac/Form/collections.form.html.twig' %}

<div class="row" id="autresComposantesPostesCollection"
	data-prototype="{{ form_widget(form.autresComposantesPostes.vars.prototype)|e('html_attr') }}">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<table class="table table-bordered">
			<thead>
				<tr>
					<th style="width: 30%">Autres composantes du poste</th>
					<th style="width: 10%" class="text-center">COMPOSANTE FAIBLE</th>
					<th style="width: 10%" class="text-center">COMPOSANTE MOYENNE</th>
					<th style="width: 10%" class="text-center">COMPOSANTE DOMINANTE</th>
					<th style="width: 35%" class="text-center">OBSERVATIONS</th>
					<th style="width: 5%" class="text-center"></th>

				</tr>
			</thead>
			<tbody>{% for autreComposantePoste in form.autresComposantesPostes %}
				{{ form_row(autreComposantePoste) }} {% endfor %}
			</tbody>
		</table>
	</div>
</div>

<div class="row">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<button
			class="btn btn-primary add_collection_link autresComposantesPostes pull-right">
			<i class="fa fa-plus"></i> Ajouter
		</button>
	</div>
</div>
