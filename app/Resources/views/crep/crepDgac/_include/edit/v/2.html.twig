{% form_theme form 'crep/crepDgac/Form/collections.form.html.twig' %}

<div class="row" id="autresCompetencesProfessionnellesCollection"
	data-prototype="{{ form_widget(form.autresCompetencesProfessionnelles.vars.prototype)|e('html_attr') }}">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<table class="table table-bordered">
			<thead>
				<tr>
					<th style="width: 30%">Autres competences professionnelles</th>
					<th style="width: 10%" class="text-center">À DÉVELOPPER</th>
					<th style="width: 10%" class="text-center">EN COURS D'ACQUISITION</th>
					<th style="width: 10%" class="text-center">MAITRISÉE</th>
					<th style="width: 35%" class="text-center">OBSERVATIONS</th>
					<th style="width: 5%" class="text-center"></th>

				</tr>
			</thead>
			<tbody>{% for autreCompetenceProfessionnelle in form.autresCompetencesProfessionnelles %}
				{{ form_row(autreCompetenceProfessionnelle) }} {% endfor %}
			</tbody>
		</table>
	</div>
</div>

<div class="row">
	<div class="col-md-12 col-sm-12 col-xs-12">
		<button
			class="btn btn-primary add_collection_link autresCompetencesProfessionnelles pull-right">
			<i class="fa fa-plus"></i> Ajouter
		</button>
	</div>
</div>
