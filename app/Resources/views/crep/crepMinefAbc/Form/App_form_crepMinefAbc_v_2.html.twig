{% extends "crep/crepMinefAbc/Form/App_form_crepMinefAbc.html.twig" %}

{% block form_errors -%}
    {% spaceless %}
    	{% if errors|length > 0 -%}
        <div class="has-error">
        	{%- for error in errors -%}
                <div class="col-sm-12">
    				<label class="control-label has-error" for="inputError"><i class="fa fa-times-circle"></i> {{ error.message }}</label>
    			</div>    		
            {%- endfor -%}
            
    	</div>
        {%- endif -%}
    {% endspaceless %}
{%- endblock form_errors %}
