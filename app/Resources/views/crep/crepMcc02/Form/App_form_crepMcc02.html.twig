{% use "Form/App_bootstrap_3_horizontal_layout.html.twig" %}


{% block _crep_mcc_objectifsEvalues_entry_widget %}
    <tr>
        <td>{{ form_row(form.libelle) }}</td>
        <td>{{ form_row(form.indicateurs)}}</td>
        <td>{{ form_row(form.resultat) }}</td>
        <td>{{ form_row(form.resultatObtenu) }}</td>
        <td class="text-center"><button class="btn btn-danger btn-xs remove-from-collection"><i class="fa fa-close"></i></button></td>
    </tr>
{% endblock %}


